backends:{backend:{name:"trillian"  backend_spec:"{{ tas_single_node_trillian_logserver_pod }}-pod:{{ tas_single_node_trillian_logserver_port_rpc }}"}}  
log_configs:{
{% for log in tas_single_node_ctlog.sharding_config %}
    config:{
        log_id: {{ log.treeid | default(trillian_tree_id) }},
        prefix: "{{ log.prefix | default(tas_single_node_fulcio.ct_log_prefix) }}"
        roots_pem_file: "{{ log.roots_pem_file | default('/ctfe-keys/fulcio-0') }}"
        private_key:{
            [type.googleapis.com/keyspb.PEMKeyFile]:{ 
                path: "{{ log.private_key | default(ctlog_private_key_pathlog.private_key) }}"
                password: "{{ log.password | default(tas_single_node_ctlog_ca_passphrase) }}" 
        ext_key_usages:"CodeSigning"  log_backend_name:"trillian"}
{% endfor %}
}