kind: Secret
apiVersion: v1
metadata:
  name: '{{ secret_name }}'
  namespace: '{{ secret_namespace }}'
data:
{%- for x in keyLists -%}
{% if x is string %}{% set x = [x] %}{% endif %}
{%- set oidx = loop.index0 -%}
{%- for keys in x -%}
{%- set iidx = loop.index0 %}

  {{ keyName[oidx] }}-{{ iidx }}: '{{ (slurpedCerts.results | selectattr('source', 'equalto' , keys ) | list | first).content }}'
{%- endfor -%}
{%- endfor -%}